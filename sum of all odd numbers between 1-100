#include <stdio.h>

int main() {
    int sum = 0;

    // Calculate the sum of odd numbers between 1 and 100
    for (int i = 1; i <= 100; i++) {
        if (i % 2 != 0) { // Check if the number is odd
            sum += i; // Add the odd number to the sum
        }
    }

    // Display the result
    printf("The sum of all odd numbers between 1 and 100 is: %d\n", sum);

    return 0;
}
